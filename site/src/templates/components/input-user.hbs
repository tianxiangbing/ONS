<div class="input-uname">
<i class="iconfont">&#xe61b;</i>
{{#if isReadOnly}}
{{input value=username readonly="readonly" class="txt-input" placeholder="邮箱/手机号码"}}
{{else}}
{{input value=username class="txt-input" placeholder="邮箱/手机号码"}}
{{/if}}
</div>
<div class="input-pwd">
<i class="iconfont">&#xe60e;</i>
{{#if islook}}
<i class="iconfont pwd-look acitve" {{action "lookpwd" on="click"}}>&#xe616;</i>
{{input type="text" value=password class="txt-input" placeholder="6-18位密码"}}
{{else}}
<i class="iconfont pwd-look" {{action "lookpwd" on="click"}}>&#xe617;</i>
{{input type="password" value=password class="txt-input" placeholder="6-18位密码"}}
{{/if}}
</div>
<button  {{bind-attr class=classname}}  {{action "click"}}>{{btnText}}</button>